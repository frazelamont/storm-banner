{
  "name": "storm-banner",
  "version": "1.0.0",
  "description": "Dismissible message banner saved to either sessionStorage, localStorage, or cookies. For cookie messages or any one-time notification.",
  "author": "stormid",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stormid/storm-banner.git"
  },
  "bugs": {
    "url": "https://github.com/stormid/storm-banner/issues"
  },
  "main": "dist/index.js",
  "keywords": [
    "stormid",
    "ui",
    "component",
    "banner",
    "session",
    "cookie",
    "localStorage",
    "sessionStorage"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "build": "node_modules/.bin/gulp build",
    "serve": "node_modules/.bin/gulp server",
    "start": "node_modules/.bin/gulp server",
    "test": "istanbul cover _mocha --report lcovonly -- -R spec && codecov",
    "test:watch": "NODE_ENV=test mocha --watch"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.23.2",
    "browserify": "^13.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-gh-pages": "^0.5.4",
    "gulp-header": "^1.8.9",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.15.0",
    "gulp-uglify": "^2.0.0",
    "gulp-wrap-umd": "^0.2.1",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^9.8.0",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.5.3",
    "object-assign": "^4.0.1",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "run-sequence": "^1.1.0",
    "should": "^11.1.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^1.1.2"
  }
